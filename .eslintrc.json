{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 15
    },
    "rules": {
        "indent": ["error", 4],
        
        "no-useless-escape":"warn",
        "no-mixed-operators":"warn",
        "no-restricted-globals":"warn",
        "radix":"warn",
        "no-sequences":"warn",
        "max-len":"warn",
        "no-bitwise":"warn",
        "default-case":"warn",
        "no-console":"warn",
        "no-unused-vars":"warn",
        "no-param-reassign":"warn",
        "no-plusplus":"warn",
        "no-await-in-loop":"warn",
        "no-restricted-syntax":"warn",
        "eqeqeq":"warn",
        "consistent-return":"warn",
        "global-require":"warn",
        "eslint-plugin-import/no-extraneous-dependencies":"warn",
        "import/no-extraneous-dependencies":"warn"
    }
}
